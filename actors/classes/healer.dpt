ACTOR Healer_HandGun : weapon
{
	Weapon.AmmoType "Cell"
	Weapon.AmmoUse 1
	Weapon.KickBack 10
	Weapon.AmmoType2 "Cell"
	Weapon.AmmoUse2 2
	Weapon.UpSound "WeaponSelect"
	Decal RailGunScorch
	Inventory.PickupMessage "Picked up a healer's pistol."
	Obituary "%o was a bit melted by healer's pistol."
	+WEAPON.NOAUTOFIRE
	+INVENTORY.UNDROPPABLE
	States
	{
	Ready:
		PIS1 A 4 A_WeaponReady
		PIS1 A 0 A_TakeInventory("IonBurst",1)
		Loop
	Deselect:
		PIS1 A 0 A_Lower
		PIS1 A 1 A_Lower
		Loop
	Select:
		PIS1 A 0 A_Raise
		PIS1 A 1 A_Raise
		Loop
	Fire:
		PIS1 B 0 A_JumpIfInventory("IonBurst",1,"Fire2")
		PIS1 B 0 A_PlaySound("weapons/plasmaf")
		PIS1 B 0 A_FireBullets(1,0,1,5,"IonBallPuff",1)
		PIS1 B 5 A_GunFlash
		PIS1 C 6 A_GiveInventory("IonBurst",1)
		PIS1 C 4 A_ReFire
		Goto Ready
	Fire2:
		PIS1 B 0 A_FireBullets(3,3,1,5,"IonBallPuff",1)
		PIS1 B 5 A_GunFlash
		PIS1 C 6 
		PIS1 C 4 A_ReFire
		Goto Ready
	AltFire:
		PIS1 B 0 A_JumpIfInventory("IonBurst",1,"AltFire2")
		PIS1 B 0 A_FireCustomMissile("IonBall")
		PIS1 B 5 A_GunFlash
		PIS1 C 10 A_GiveInventory("IonBurst",1)
		PIS1 C 4 A_ReFire
		Goto Ready
	AltFire2:
		PIS1 B 0 A_FireCustomMissile("IonBall",random(3,-3),1)
		PIS1 B 5 A_GunFlash
		PIS1 C 10 A_GiveInventory("IonBurst",1)
		PIS1 C 4 A_ReFire
		Goto Ready
	Flash:
		PSF1 A 3 Bright A_Light2
		PSF1 A 2 Bright A_Light1
		Goto LightDone
	}
}
