ACTOR Healer : PlayerPawn 10003
{
	Speed 1
	Health 100
	Radius 16
	Height 56
	Mass 100
	PainChance 255
	Player.DisplayName "Healer"
	Player.StartItem "isHealer"
	Player.StartItem "Curing_Cannon"
	Player.StartItem "HealthPack"
	Player.StartItem "HealthPacks", 10
	Player.StartItem "Healer_HandGun"
	Player.StartItem "AntiTechgrenade", 5
	Player.StartItem "Hands"
	Player.StartItem "Cell", 96
	Player.ColorRange 112, 127
	DamageFactor "Normal", 0.9
	DamageFactor "Slime", 3
	States
	{
	Spawn:
		PBAK A 150
		CRE2 A 0 A_JumpIf (z-floorz>0, "Spawn")
		CRE2 A 0 A_JumpIfInventory("Health", 100, "Spawn")
		CRE2 A 0 ACS_Execute(988,0)
		Loop
	See:
		PBAK ABCD 4 
		Loop
	Missile:
		PBAK E 12 
		Goto Spawn
	Melee:
		PBAK F 6 BRIGHT
		Goto Missile
	Pain:
		PBAK G 4 
		PBAK G 4 A_Pain
		Goto Spawn
	Death:
		PBAK H 10
		PBAK I 10 A_PlayerScream
		PBAK J 10 A_NoBlocking
		PBAK KLM 10
		PBAK N 1 A_FadeOut(0.02)
		PBAK N 0 A_SpawnItemEx("HealerCorpse", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		PBAK N 1
		PBAK N -1
		Stop
	XDeath:
		PBAK O 5
		PBAK P 5 A_XScream
		PBAK Q 5 A_NoBlocking
		PBAK RSTUV 5
		PBAK W -1
		Stop
	}
}
ACTOR HealerCorpse
{
	Radius 16
	Height 56
	health 100
	speed 12
	painchance 0
	MONSTER
	MeleeDamage 255
	states
	{
	Spawn:
		TNT1 A 0 
		TNT1 A 1 A_Wander
		TNT1 A 1 Thing_Damage(0,255,23)
		Loop
	See:
		PBAK A 2 ThrustThingZ(0,33,4,0) 
		PBAK AAAABBBB 1 A_FastChase
		PBAK CCCCDDDD 1 A_FastChase
		PBAK AAAABBBB 1 A_FastChase
		TNT1 A 0 A_Jump(127, "JumpForward", "JumpBackward")
		Loop
	JumpForWard:
		PBAK A 0 A_Recoil(0.554) 
		PBAK A 0 ThrustThingZ(0,33,4,0)
		PBAK A 0 A_Recoil(0.554)
		PBAK A 1 A_Recoil(0.554)
		Loop
	JumpBackward:
		PBAK A 0 A_Recoil(-0.554)
		PBAK A 0 ThrustThingZ(1,33,5,0)
		PBAK A 0 A_Recoil(-0.554)
		PBAK A 1 A_Recoil(-0.554)
		Loop
	Missile:
		PBAK E 6 A_FaceTarget
		PBAK F 6 A_CustomBulletAttack(2,1,1,127,"ShockPuff")
		Goto See
	Melee:
		PBAK E 6 A_FaceTarget
		PBAK F 6 A_MeleeAttack
		Goto See
	Death.Railgun:
	XDeath.Railgun:
		PBAK W 1
		PBAK W 1 A_NoBlocking
		PBAK W -1
		stop
	XDeath: 
	Death:
		PBAK H 10
		PBAK I 10 A_Scream
		PBAK J 10 A_NoBlocking
		PBAK KLM 10
		PBAK N 0
		Stop
	Raise:
		PBAK NMLKJIH 3
		goto See
	}
}
ACTOR Curing_Cannon : Weapon 
{
	Weapon.AmmoUse1 1
	Weapon.AmmoGive1 96
	Weapon.AmmoType1 "Cell"
	Weapon.AmmoUse2 1
	Weapon.AmmoType2 "Cell"
	Weapon.UpSound "WeaponSelect"
	states
	{
	Ready:
		REP4 A 0
		REP4 A 1 A_weaponready
		loop
	Select:
		REP4 A 1 A_raise
		loop
	Deselect:
		REP4 A 1 A_lower
		loop
	Fire:
		REP4 A 0 A_JumpIfTargetInLOS ("AltFire")
		goto Ready
	AltFire:
		REP4 A 4
		REP4 BC 4 bright A_FireCustomMissile("HealthMissile",0,1,0,0)
		REP4 B 0 A_PlaySound("Healing")
		REP4 A 2 A_Refire
		goto Ready
	Spawn:
		PIST A -1
		stop
	}
}
